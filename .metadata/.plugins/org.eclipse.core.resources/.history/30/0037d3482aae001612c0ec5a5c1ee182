package StringHandDemo;

public class StringCom {

	

	public static void main(String[] args) {

            String s="Nalini";
            
            char[] ch=s.toCharArray();
            
            for(int i=0;i<=s.length()-1;i++)
            {
            	for(int j=i+1;j<=i;j++)
            	{
            		if(ch[j]<ch[j-1])
            		{
            			char temp=ch[j-1];
            			ch[j-1]=ch[j];
            			ch[j]=temp;
            		}
            	}
            }
            
	}

}
