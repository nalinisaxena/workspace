package AppletDemo;

import java.applet.Applet;
import java.awt.Graphics;

public class Micky extends Applet{
	
	public void paint( Graphics g)
	
	{
		g.drawOval(5, 6, 500, 400);
		
		g.drawOval(150, 150, 200,200);
		
		g.drawOval(73,150, 90, 80);
		
		g.drawOval(335,150, 90, 80);
		
		g.drawOval(210, 240, 70, 50);
		
		g.drawOval(210,180, 20, 50);
		
		g.drawOval(250,180, 20, 50);
		
		g.drawOval(220,205,10,20);
		
		g.drawOval(250,205, 10,20);
		
		g.drawArc(180,200, 150, 150, 180, 160);
		
		
		
		
		
		
		
		
		
	}
	
	
	

}
