package StringHandDemo;

public class equalsdemo {

	

	public static void main(String[] args) {

                  String s1="Nalini";
                  
                  String s2="Mala";
                  
                  String s3=new String("Nalini");
                  
                  System.out.println(s1==s2);
                  
                  System.out.println(s1==s3);
	}

}
