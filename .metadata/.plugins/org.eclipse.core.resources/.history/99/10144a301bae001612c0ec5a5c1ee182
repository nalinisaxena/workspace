package StringHandDemo;

public class equalsdemo {

	

	public static void main(String[] args) {

                  String s1="Nalini";
                  
                  String s2="Nalini";
                  
                  String s3=new String("Nalini");
                  
                  System.out.println(s1.equals(s2));
                  
                  System.out.println(s1.equals(s3));
	}

}
